layout {
    pane split_direction="vertical" {
		pane command="cling" {
				name "C/C++"
		}
		pane command="docker" {
				name "C#"
				args "run" "-it" "--rm" "mono" "csharp"
		}
		pane command="docker" {
				name "Dart"
				args "run" "-it" "--rm" "dart"
		}
    }
    pane split_direction="vertical" {
        pane command="gore" {
				name "Go"
		}
		pane command="docker" {
				name "Java"
				args "run" "-it" "--rm" "openjdk"
		}
        pane command="kotlinc" {
				name "Kotlin"
		}
    }
    pane split_direction="vertical" {
		pane command="docker" {
				name "JavaScript/TypeScript"
				args "run" "-it" "--rm" "denoland/deno" "repl"
		}
		pane command="evcxr" {
				name "Rust"
		}
		pane command="swift" {
				name "Swift"
				args "repl"
		}
    }
}
